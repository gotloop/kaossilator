{
  "name": "kaossilator",
  "version": "1.0.0",
  "description": "a tone.js/react recreation of the Korg Kaossillator",
  "main": "index.js",
  "scripts": {
    "build": "webpack && npm run css && npm run html",
    "test": "mocha --compilers js:babel-register --recursive --reporter bdd",
    "dev": "webpack-dev-server --progress --colors --content-base www/",
    "lint": "eslint app",
    "postinstall": "webpack",
    "css":"node-sass app/style.scss www/style.css",
    "html":"pug app --out www/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gotloop/kaossilator.git"
  },
  "author": "jibhaine",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gotloop/kaossilator/issues"
  },
  "homepage": "https://github.com/gotloop/kaossilator#readme",
  "dependencies": {
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "tone": "^0.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "^2.8.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-react": "^5.0.1",
    "node-sass": "^3.5.3",
    "pug": "^2.0.0-alpha6",
    "pug-cli": "^1.0.0-alpha1",
    "sass-loader": "^3.2.0",
    "uglify-loader": "^1.3.0",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1"
  }
}
